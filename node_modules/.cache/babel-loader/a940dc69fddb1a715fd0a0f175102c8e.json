AIA,QAAA,OAAA,EAAA,KAAA,YAAA,CAAA,OAJA;AAKA,QAAA,iBAAA,EAAA,KAAA,UALA;AAMA,QAAA,kBAAA,EAAA,SANA;AAOA,QAAA,mBAAA,EAAA;AAPA,OAAA;AAUA,MAAA,KAAA,CACA,IADA,CACA,uCADA,EACA,YADA,EAEA,IAFA,CAEA;AAAA,eAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,UAAA,CAAA;AAAA,OAFA,EAGA;AAHA,OAIA,KAJA,CAIA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OAJA;AAKA;AA3BA,GAhBA;AA6CA,EAAA,OA7CA,qBA6CA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,eAAA,CAAA,EAAA;AACA,UAAA;AACA,aAAA,aAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,eAAA,CAAA,CAAA;AACA,OAFA,CAEA,OAAA,CAAA,EAAA;AACA,QAAA,YAAA,CAAA,UAAA,CAAA,eAAA;AACA;AACA;AACA,GArDA;AAsDA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,aAAA,KAAA,aAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA,IAAA,EAAA;AACA,eAAA,UAAA,CAAA,KAAA,CAAA,GAAA,UAAA,CAAA,IAAA,CAAA,KAAA,CAAA;AACA,OAFA,EAEA,CAFA,CAAA;AAGA,KALA;AAMA,IAAA,UANA,wBAMA;AACA,aAAA,KAAA,UAAA,GAAA,IAAA;AACA;AARA;AAtDA,CAAA","sourcesContent":["<template>\n  <div class=\"shopping\">\n    <Header />\n    <!-- Breadcrumb -->\n    <div class=\"breacrumb-section text-left\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-lg-12\">\n            <div class=\"breadcrumb-text product-more\">\n              <router-link to=\"/\">\n                <i class=\"fa fa-home\"></i> Holic\n              </router-link>\n              <span>Shopping Cart</span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- Breadcrumb end -->\n\n    <!-- Shopping Cart Section Begin -->\n    <section class=\"shopping-cart spad\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-lg-8\">\n            <div class=\"row\">\n              <div class=\"col-lg-12\">\n                <div class=\"cart-table\">\n                  <table>\n                    <thead>\n                      <tr>\n                        <th>Image</th>\n                        <th class=\"p-name text-center\">Product Name</th>\n                        <th>Price</th>\n                        <th>Action</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      <tr v-for=\"keranjang in keranjangUser\" :key=\"keranjang.id\">\n                        <td class=\"cart-pic first-row\">\n                          <img class=\"img-cart\" :src=\"keranjang.photo\" />\n                        </td>\n                        <td class=\"cart-title first-row text-center\">\n                          <h5>{{ keranjang.name }}</h5>\n                        </td>\n                        <td class=\"p-price first-row\">Rp {{ keranjang.price }}.000</td>\n                        <td @click=\"removeItem(keranjangUser.index)\" class=\"delete-item\">\n                          <a href=\"#\">\n                            <i class=\"material-icons\">close</i>\n                          </a>\n                        </td>\n                      </tr>\n                    </tbody>\n                  </table>\n                </div>\n              </div>\n              <div class=\"col-lg-8\">\n                <h4 class=\"mb-4 text-left\">Informasi Pembeli:</h4>\n                <div class=\"user-checkout text-left\">\n                  <form>\n                    <div class=\"form-group\">\n                      <label for=\"namaLengkap\">Nama lengkap</label>\n                      <input\n                        type=\"text\"\n                        class=\"form-control\"\n                        id=\"namaLengkap\"\n                        aria-describedby=\"namaHelp\"\n                        placeholder=\"Masukan Nama\"\n                        v-model=\"customerInfo.name\"\n                      />\n                    </div>\n                    <div class=\"form-group\">\n                      <label for=\"namaLengkap\">Email Address</label>\n                      <input\n                        type=\"email\"\n                        class=\"form-control\"\n                        id=\"emailAddress\"\n                        aria-describedby=\"emailHelp\"\n                        placeholder=\"Masukan Email\"\n                        v-model=\"customerInfo.email\"\n                      />\n                    </div>\n                    <div class=\"form-group\">\n                      <label for=\"namaLengkap\">No. HP</label>\n                      <input\n                        type=\"text\"\n                        class=\"form-control\"\n                        id=\"noHP\"\n                        aria-describedby=\"noHPHelp\"\n                        placeholder=\"Masukan No. HP\"\n                        v-model=\"customerInfo.number\"\n                      />\n                    </div>\n                    <div class=\"form-group\">\n                      <label for=\"alamatLengkap\">Alamat Lengkap</label>\n                      <textarea\n                        class=\"form-control\"\n                        id=\"alamatLengkap\"\n                        rows=\"3\"\n                        v-model=\"customerInfo.address\"\n                      ></textarea>\n                    </div>\n                  </form>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"col-lg-4\">\n            <div class=\"row\">\n              <div class=\"col-lg-12\">\n                <div class=\"proceed-checkout text-left\">\n                  <ul>\n                    <li class=\"mb-4\" style=\"font-weight: bold;\">Ringkasan Pesanan</li>\n                    <li class=\"subtotal mt-3\">\n                      Total Recurring\n                      <span>Rp {{ totalHarga }}.000</span>\n                    </li>\n                    <li class=\"subtotal mt-3\">\n                      Ongkir\n                      <span>Rp 10.000</span>\n                    </li>\n                    <li class=\"subtotal mt-3\">\n                      Total Today\n                      <span>Rp {{ totalBiaya }}.000</span>\n                    </li>\n                    <li class=\"mt-4\" style=\"font-weight: bold;\">Payment</li>\n                    <li class=\"subtotal mt-3\">\n                      Bank Transfer\n                      <span>Mandiri</span>\n                    </li>\n                    <li class=\"subtotal mt-3\">\n                      No. Rekening\n                      <span>2208 1996 1403</span>\n                    </li>\n                    <li class=\"subtotal mt-3\">\n                      Nama Pengirim\n                      <span>Workaholic</span>\n                    </li>\n                  </ul>\n                  <!-- <router-link to=\"/success\"> -->\n                  <a @click=\"checkout()\" href=\"#\" class=\"proceed-btn\">I ALREADY PAID</a>\n                  <!-- </router-link> -->\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n    <!-- Shopping Cart Section End -->\n  </div>\n</template>\n\n<script>\nimport Header from \"@/components/Header.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"Cart\",\n  components: {\n    Header\n  },\n  data() {\n    return {\n      keranjangUser: [],\n      customerInfo: {\n        name: \"\",\n        email: \"\",\n        number: \"\",\n        address: \"\"\n      }\n    };\n  },\n  methods: {\n    removeItem(index) {\n      this.keranjangUser.splice(index, 1);\n      const parsed = JSON.stringify(this.keranjangUser);\n      localStorage.setItem(\"keranjangUser\", parsed);\n    },\n    // Fungsi mengirim data ke API\n    checkout() {\n      let productIds = this.keranjangUser.map(function(product) {\n        return product.id;\n      });\n\n      let checkoutData = {\n        name: this.customerInfo.name,\n        email: this.customerInfo.email,\n        number: this.customerInfo.number,\n        address: this.customerInfo.address,\n        transaction_total: this.totalBiaya,\n        transaction_status: \"PENDING\",\n        transaction_details: productIds\n      };\n\n      axios\n        .post(\"http://holicend.masuk.id/api/checkout\", checkoutData)\n        .then(() => this.$router.push(\"congrats\"))\n        // eslint-disable-next-line no-console\n        .catch(err => console.log(err));\n    }\n  },\n  mounted() {\n    if (localStorage.getItem(\"keranjangUser\")) {\n      try {\n        this.keranjangUser = JSON.parse(localStorage.getItem(\"keranjangUser\"));\n      } catch (e) {\n        localStorage.removeItem(\"keranjangUser\");\n      }\n    }\n  },\n  computed: {\n    totalHarga() {\n      return this.keranjangUser.reduce(function(items, data) {\n        return parseFloat(items) + parseFloat(data.price);\n      }, 0);\n    },\n    totalBiaya() {\n      return this.totalHarga + 10.0;\n    }\n  }\n};\n</script>\n\n<style scoped>\n.img-cart {\n  width: 100px;\n  height: 100px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}