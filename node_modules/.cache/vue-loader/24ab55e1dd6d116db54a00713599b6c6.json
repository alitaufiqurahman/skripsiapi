          v-for=\"itemProduct in products\"\n              v-bind:key=\"itemProduct.id\"\n            >\n              <div class=\"pi-pic\">\n                <img src=\"img/x10.jpg\" alt=\"\" />\n                <ul>\n                  <li class=\"w-icon active\">\n                    <a href=\"#\"><i class=\"icon_bag_alt\"></i></a>\n                  </li>\n                  <li class=\"quick-view\">\n                    <router-link v-bind:to=\"'/product/' + itemProduct.id\"\n                      >+ Lihat Tas</router-link\n                    >\n                  </li>\n                </ul>\n              </div>\n              <div class=\"pi-text\">\n                <div class=\"catagory-name\">{{ itemProduct.type }}</div>\n                <router-link to=\"/product\">\n                  <a href=\"#\">\n                    <h5>{{ itemProduct.name }}</h5>\n                  </a>\n                </router-link>\n                <div class=\"product-price\">IDR {{ itemProduct.price }}.000</div>\n              </div>\n            </div>\n            <div\n              class=\"product-item\"\n              v-for=\"itemProduct in products\"\n              v-bind:key=\"itemProduct.id\"\n            >\n              <div class=\"pi-pic\">\n                <img\n                  src=\"img/0B721641-ADC2-47C4-B1E2-2E10C253A33E_1_105_c.jpeg\"\n                  alt=\"\"\n                />\n                <ul>\n                  <li class=\"w-icon active\">\n                    <a href=\"#\"><i class=\"icon_bag_alt\"></i></a>\n                  </li>\n                  <li class=\"quick-view\">\n                    <router-link v-bind:to=\"'/product/' + itemProduct.id\"\n                      >+ Lihat Tas</router-link\n                    >\n                  </li>\n                </ul>\n              </div>\n              <div class=\"pi-text\">\n                <div class=\"catagory-name\">{{ itemProduct.type }}</div>\n                <router-link to=\"/product\">\n                  <a href=\"#\">\n                    <h5>{{ itemProduct.name }}</h5>\n                  </a>\n                </router-link>\n                <div class=\"product-price\">IDR {{ itemProduct.price }}.000</div>\n              </div>\n            </div>\n          </carousel>\n        </div>\n      </div>\n    </div>\n  </section>\n  <!-- Holicbag -->\n</template>\n\n<script>\nimport carousel from 'vue-owl-carousel';\nimport axios from 'axios';\n\nexport default {\n  name: 'Holicbag',\n  components: {\n    carousel,\n  },\n  data() {\n    return {\n      products: [],\n    };\n  },\n  mounted() {\n    axios\n      .get('http://holicend.masuk.id/api/products')\n      .then((res) => (this.products = res.data.data.data))\n      // eslint-disable-next-line no-console\n      .catch((err) => console.log(err));\n  },\n};\n</script>\n\n<style scoped>\n.product-item {\n  margin-right: 25px;\n}\n</style>\n"]}]}